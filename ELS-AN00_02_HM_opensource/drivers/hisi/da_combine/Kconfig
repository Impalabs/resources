config DA_COMBINE_V3_CODEC
	tristate "Support DA_combine_v3 Codec IC"
	default n
	help
          Enables the DA_COMBINE_V3 core driver.

config HAC_SUPPORT
	bool "Support HAC for DA_combine_v3 Codec"
	depends on DA_COMBINE_V3_CODEC
	default n
	help
          Enables hac for DA_COMBINE_V3 Codec.

config RCV_TDD_SUPPORT
	bool "Support RCV TDD for DA_combine_v3 Codec"
	depends on DA_COMBINE_V3_CODEC
	default n
	help
          Enables rcv tdd for DA_COMBINE_V3 Codec.
