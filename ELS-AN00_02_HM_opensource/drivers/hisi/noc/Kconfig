config HISI_NOC
	depends on HISI_BB
	bool "Support NOC"
	default n
	help
	  This driver supports XXX NOC,
	  including noc driver

config HISI_NOC_DBG
	depends on HISI_NOC && HISI_DEBUG_FS
	bool "Support NOC Dbg node"
	default n
	help
	  Support NOC Dbg node
config HISI_NOC_MODID_REGISTER
	depends on HISI_NOC
	bool "Support NOC modid register"
	default n
	help
	  Support NOC modid register
config HISI_QIC
	depends on HISI_BB
	bool "Support QIC"
	default n
	help
	  This driver supports XXX QIC
config HISI_QIC_MODID_REGISTER
        depends on HISI_QIC
        bool "Support QIC modid register"
        default n
        help
          Support QIC modid register

